<% unless @movement_lineitem.errors %>
	$("#info_block").html("количество обновлено");
<% else %>
	$("#info_block").html("<%= j render(:partial => 'cp/shared/errors', :locals => { entity: @movement_lineitem } ) %>");
	$("#info_block .alert").slideToggle();

	setTimeout(function(){
		$("#info_block .alert").slideToggle();
	}, 1500);
	setTimeout(function(){
		$("#info_block .alert").remove();
	}, 2200);
	
<% end %>
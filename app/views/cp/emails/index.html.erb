<h1>Подписчики</h1>

<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th colspan="2">email</th>
    </tr>
  </thead>

  <tbody>
    <% @emails.each do |email| %>
      <tr>
        <td><%= email.id %></td>
        <td><%= email.email %></td>
        <td><%= link_to 'удалить', cp_email_path(email), method: :delete, data: { confirm: 'Вы уверены?' }, class: "text-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
